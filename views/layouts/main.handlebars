<script type="text/javascript">
    $(document).ready(function(){
        (function($){
            $('#Search').keyup(function(){
                var rex=new RegExp($(this).val(),'i');
                $('.searchable tr').hide();
                $('.searchable tr').filter(function(){
                    return rex.test($(this).text());}).show();
            })
        }(jQuery));
    })
</script>
